<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Meaow"
    android:orientation="vertical"
    android:background="#00000000">


    <androidx.fragment.app.FragmentContainerView
        android:layout_above="@id/meowep"
        android:id="@+id/containero"
        android:layout_width="match_parent"
        android:layout_height="match_parent"></androidx.fragment.app.FragmentContainerView>

    <com.etebarian.meowbottomnavigation.MeowBottomNavigation
        android:background="#00000000"
        android:layout_width="match_parent"
        android:id="@+id/meowep"
        android:layout_gravity="bottom"
        android:layout_alignParentBottom="true"
        android:layout_height="wrap_content"
        app:mbn_circleColor="#3F51B5"
        app:mbn_backgroundBottomColor="#FFFFFF"
        app:mbn_countBackgroundColor="#ff6f00"
        app:mbn_countTextColor="#ffffff"
        app:mbn_defaultIconColor="#90a4ae"
        app:mbn_rippleColor="#2FFFFFFF"
        app:mbn_selectedIconColor="@color/bot"
        app:mbn_shadowColor="#1f212121"
        />

</RelativeLayout>