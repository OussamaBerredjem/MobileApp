<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1000">
       <KeyFrameSet>
       </KeyFrameSet>
        <OnClick />
        <OnSwipe />
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:id="@+id/textView4"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toBottomOf="@+id/circleImageView"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="8dp"
            motion:pathMotionArc="startHorizontal"
            motion:layout_constraintVertical_bias="0.044"
            motion:layout_constraintHorizontal_bias="0.498"
            motion:layout_constraintBottom_toTopOf="@+id/linearLayout4" />
        <Constraint
            android:id="@+id/circleImageView"
            android:layout_width="90dp"
            android:layout_height="90dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="0.099"
            motion:layout_constraintTop_toTopOf="parent"
            motion:pathMotionArc="startHorizontal"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintHorizontal_bias="0.498" />
        <Constraint
            motion:layout_editor_absoluteY="1dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:id="@+id/linearLayout3"
            motion:layout_constraintEnd_toEndOf="parent" />
        <Constraint
            motion:layout_constraintTop_toTopOf="parent"
            android:id="@+id/imageView2"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_editor_absoluteX="16dp"
            motion:layout_constraintBottom_toBottomOf="parent" />
        <Constraint
            motion:layout_constraintVertical_bias="0.023"
            android:layout_height="wrap_content"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintHorizontal_bias="0.043"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:id="@+id/circleImageView2" />
        <Constraint
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/linearLayout4"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toBottomOf="@+id/circleImageView"
            android:layout_marginTop="230dp"
            motion:layout_constraintBottom_toBottomOf="parent" />
        <Constraint
            motion:layout_constraintBottom_toBottomOf="parent"
            android:id="@+id/linearLayout5"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_editor_absoluteY="731dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="411dp"
            android:layout_marginStart="411dp" />
        <Constraint
            motion:layout_constraintBottom_toBottomOf="parent"
            android:id="@+id/linearLayout6"
            android:layout_marginEnd="411dp"
            motion:layout_editor_absoluteY="730dp"
            android:layout_marginRight="411dp"
            motion:layout_constraintEnd_toEndOf="parent" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:id="@+id/circleImageView"
            android:layout_width="70dp"
            android:layout_height="60dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="0.021"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toEndOf="@+id/circleImageView2" />
        <Constraint
            android:id="@+id/circleImageView"
            android:layout_width="70dp"
            android:layout_height="60dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="0.021"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toEndOf="@+id/circleImageView2" />
        <Constraint
            motion:layout_constraintVertical_bias="0.0"
            android:layout_height="38dp"
            android:layout_marginTop="28dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintHorizontal_bias="0.0"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:id="@+id/textView4"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toEndOf="@+id/circleImageView" />
        <Constraint
            motion:layout_constraintVertical_bias="0.023"
            android:layout_height="wrap_content"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintHorizontal_bias="0.045"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:id="@+id/circleImageView2" />
        <Constraint
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:id="@+id/linearLayout4"
            motion:layout_constraintTop_toBottomOf="@+id/textView4"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="120dp" />
    </ConstraintSet>
</MotionScene>