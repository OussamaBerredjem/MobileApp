<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1042">
       <KeyFrameSet>
       </KeyFrameSet>
        <OnSwipe motion:maxAcceleration="0.1"
            motion:maxVelocity="0.1" />
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:id="@+id/textView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="368dp"
            motion:transitionEasing="linear"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:pathMotionArc="startVertical"
            motion:layout_constraintTop_toBottomOf="@+id/imageView"
            motion:layout_constraintVertical_bias="0.064"
            motion:layout_constraintHorizontal_bias="0.63" />
        <Constraint
            android:layout_marginEnd="125dp"
            android:layout_height="120dp"
            android:layout_marginTop="102dp"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_marginBottom="599dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintHorizontal_bias="0.465"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="300dp"
            motion:layout_constraintVertical_bias="1.0"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="140dp"
            android:id="@+id/imageView"
            motion:pathMotionArc="startVertical" />
        <Constraint
            android:layout_width="391dp"
            android:layout_height="554dp"
            android:id="@+id/linearLayout2"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent" />
        <Constraint
            motion:layout_editor_absoluteY="275dp"
            motion:layout_constraintEnd_toEndOf="parent"
            android:id="@+id/space"
            motion:layout_constraintStart_toStartOf="parent" />
        <Constraint
            motion:layout_constraintEnd_toEndOf="parent"
            android:id="@+id/space2"
            motion:layout_editor_absoluteX="393dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintTop_toTopOf="parent" />
        <Constraint
            motion:layout_constraintEnd_toEndOf="parent"
            android:id="@+id/space3"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginBottom="532dp"
            motion:layout_constraintBottom_toTopOf="@+id/imageView"
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_width="wrap_content"
            motion:layout_constraintHorizontal_bias="0.127" />
        <Constraint
            motion:layout_constraintEnd_toEndOf="parent"
            android:id="@+id/space4"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toBottomOf="@+id/textView3" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:layout_height="70dp"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_width="70dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="40dp"
            android:id="@+id/imageView" />
        <Constraint
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="155dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:id="@+id/textView3"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_marginStart="200dp"
            android:layout_marginTop="16dp" />
        <Constraint
            android:layout_height="554dp"
            motion:layout_editor_absoluteX="1dp"
            android:layout_width="391dp"
            motion:layout_constraintTop_toBottomOf="@+id/textView3"
            android:id="@+id/linearLayout2"
            motion:layout_editor_absoluteY="274dp" />
        <Constraint
            android:layout_width="391dp"
            android:layout_height="554dp"
            motion:layout_editor_absoluteY="274dp"
            motion:layout_editor_absoluteX="1dp"
            android:id="@+id/linearLayout2"
            motion:layout_constraintTop_toBottomOf="@+id/textView3"
            android:layout_marginTop="40dp" />
    </ConstraintSet>
</MotionScene>